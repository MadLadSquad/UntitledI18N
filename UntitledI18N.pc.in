prefix="@CMAKE_INSTALL_PREFIX@"
exec_prefix="${prefix}"
libdir="${prefix}/lib"
includedir="${prefix}/include"
compile_defs="@compile_defs@"

Name: @PROJECT_NAME@
Description: A cross-platform i18n library for C and C++
URL: https://github.com/MadLadSquad/UntitledI18N
Version: @PROJECT_VERSION@
Cflags: -I"${includedir}"
Libs: -L"${libdir}" -l@PROJECT_NAME@
Libs.private: -L"${libdir}" -l@PROJECT_NAME@